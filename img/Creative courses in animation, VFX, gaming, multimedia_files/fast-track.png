    $.validator.addMethod("letters", function(value, element) {
      return this.optional(element) || value == value.match(/^[a-zA-Z\s]*$/);
    });    
    $.validator.addMethod("email", function(value, element) {
      return this.optional(element) || value == value.match(/^[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,}$/);
    });
    $.validator.addMethod("numbers", function(value, element) {
      return this.optional(element) || value == value.match(/^[46789]\d{9,10}$/);
    });
    $.validator.addMethod("numberlakhs", function(value, element) {
      return this.optional(element) || value == value.match(/^[3456789]\d{1,5}$/);
